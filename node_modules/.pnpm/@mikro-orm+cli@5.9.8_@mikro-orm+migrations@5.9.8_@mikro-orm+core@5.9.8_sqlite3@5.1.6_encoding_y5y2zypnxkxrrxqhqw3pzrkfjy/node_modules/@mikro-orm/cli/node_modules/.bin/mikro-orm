#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lazycodebaker/Desktop/sayble-server/node_modules/.pnpm/@mikro-orm+cli@5.9.8_@mikro-orm+migrations@5.9.8_@mikro-orm+core@5.9.8_sqlite3@5.1.6_encoding_y5y2zypnxkxrrxqhqw3pzrkfjy/node_modules/@mikro-orm/cli/node_modules:/Users/lazycodebaker/Desktop/sayble-server/node_modules/.pnpm/@mikro-orm+cli@5.9.8_@mikro-orm+migrations@5.9.8_@mikro-orm+core@5.9.8_sqlite3@5.1.6_encoding_y5y2zypnxkxrrxqhqw3pzrkfjy/node_modules/@mikro-orm/node_modules:/Users/lazycodebaker/Desktop/sayble-server/node_modules/.pnpm/@mikro-orm+cli@5.9.8_@mikro-orm+migrations@5.9.8_@mikro-orm+core@5.9.8_sqlite3@5.1.6_encoding_y5y2zypnxkxrrxqhqw3pzrkfjy/node_modules:/Users/lazycodebaker/Desktop/sayble-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lazycodebaker/Desktop/sayble-server/node_modules/.pnpm/@mikro-orm+cli@5.9.8_@mikro-orm+migrations@5.9.8_@mikro-orm+core@5.9.8_sqlite3@5.1.6_encoding_y5y2zypnxkxrrxqhqw3pzrkfjy/node_modules/@mikro-orm/cli/node_modules:/Users/lazycodebaker/Desktop/sayble-server/node_modules/.pnpm/@mikro-orm+cli@5.9.8_@mikro-orm+migrations@5.9.8_@mikro-orm+core@5.9.8_sqlite3@5.1.6_encoding_y5y2zypnxkxrrxqhqw3pzrkfjy/node_modules/@mikro-orm/node_modules:/Users/lazycodebaker/Desktop/sayble-server/node_modules/.pnpm/@mikro-orm+cli@5.9.8_@mikro-orm+migrations@5.9.8_@mikro-orm+core@5.9.8_sqlite3@5.1.6_encoding_y5y2zypnxkxrrxqhqw3pzrkfjy/node_modules:/Users/lazycodebaker/Desktop/sayble-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
